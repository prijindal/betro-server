{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "db:clean": "ts-node src/scripts/clean",
    "db:migrate": "ts-node src/scripts/migration",
    "lint": "eslint \"**/*.{js,ts}\" --quiet --fix",
    "prettier": "prettier src test --write",
    "serve": "node dist/server.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "start": "npm run serve",
    "test": "jest --coverage --verbose --forceExit --runInBand",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon -w dist dist/server.js",
    "watch-ts-node": "nodemon -w src --exec ts-node src/server.ts",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w"
  },
  "pre-commit": [
    "lint",
    "prettier"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^9.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "express-validator": "^6.10.1",
    "faker": "^5.5.3",
    "helmet": "^4.5.0",
    "ioredis": "^4.27.1",
    "jpeg-js": "^0.4.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "pg": "^8.6.0",
    "pino": "^6.11.3",
    "postgres-migrations": "^5.1.1",
    "rate-limit-redis": "^2.1.0",
    "throttle-debounce": "^3.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.11",
    "@types/express-rate-limit": "^5.1.1",
    "@types/faker": "^5.5.3",
    "@types/ioredis": "^4.26.1",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.168",
    "@types/node": "^15.0.1",
    "@types/pg": "^7.14.11",
    "@types/pino": "^6.3.8",
    "@types/rate-limit-redis": "^1.7.1",
    "@types/supertest": "^2.0.11",
    "@types/throttle-debounce": "^2.1.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "betro-js-lib": "git+https://prijindal@github.com/betro-app/betro-js-lib.git#3a02475c7e30c6d7cf096e38c2ff0874ca600e69",
    "concurrently": "^6.0.2",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
