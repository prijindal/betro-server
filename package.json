{
  "name": "betro-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "build:docker": "npm run pkg:server && npm run pkg:clean && npm run pkg:migration && docker build -t betro/server .",
    "docker:publish": "docker push betro/server:latest",
    "pkg:server": "pkg -t node14-linux-x64 --out-path dist/linux-x64 dist/server.js",
    "pkg:clean": "pkg -t node14-linux-x64 --out-path dist/linux-x64/clean dist/scripts/clean.js",
    "pkg:migration": "pkg -t node14-linux-x64 --out-path dist/linux-x64/migration dist/scripts/migration.js",
    "db:clean": "ts-node src/scripts/clean",
    "db:migrate": "ts-node src/scripts/migration",
    "lint": "eslint \"**/*.{js,ts}\" --quiet --fix",
    "prettier": "prettier src test --write",
    "serve": "node dist/server.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "start": "npm run serve",
    "test": "jest --coverage --verbose --forceExit --runInBand",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon -w dist dist/server.js",
    "watch-ts-node": "nodemon -w src --exec ./node_modules/.bin/ts-node src/server.ts",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w"
  },
  "pre-commit": [
    "lint",
    "prettier"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.5.0",
    "express-validator": "^6.13.0",
    "express-ws": "^5.0.2",
    "faker": "^5.5.3",
    "helmet": "^4.6.0",
    "ioredis": "^4.28.0",
    "jpeg-js": "^0.4.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "pg": "^8.7.1",
    "pino": "^7.0.5",
    "postgres-migrations": "^5.3.0",
    "rate-limit-redis": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "throttle-debounce": "^3.0.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.41",
    "typeorm-typedi-extensions": "^0.4.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-ws": "^3.0.1",
    "@types/faker": "^5.5.9",
    "@types/ioredis": "^4.27.8",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.6",
    "@types/pg": "^8.6.1",
    "@types/pino": "^6.3.12",
    "@types/rate-limit-redis": "^1.7.2",
    "@types/throttle-debounce": "^2.1.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "betro-js-client": "git+https://prijindal@github.com/betro-app/betro-js-client.git#4e9018d07e0f0f18d203d65f94ed4d6432d6c9ba",
    "concurrently": "^6.3.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "pino-pretty": "^7.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
